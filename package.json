{
  "name": "canonical-cat",
  "version": "0.2.0",
  "description": "Automated catalog generator for React components and utilities with smart versioning and LLM enhancement",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "canonical-cat": "dist/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ncpleslie/canonical-cat.git"
  },
  "bugs": {
    "url": "https://github.com/ncpleslie/canonical-cat/issues"
  },
  "homepage": "https://github.com/ncpleslie/canonical-cat",
  "author": "",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "test": "vitest",
    "test:ci": "vitest --run --reporter=dot",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run"
  },
  "keywords": [
    "react",
    "components",
    "documentation",
    "catalog",
    "ai",
    "llm",
    "typescript"
  ],
  "dependencies": {
    "commander": "14.0.3",
    "fastest-levenshtein": "1.0.16",
    "glob": "13.0.2",
    "minimatch": "10.1.2",
    "ts-morph": "27.0.2",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.14",
    "@types/node": "20.19.0",
    "typescript": "5.9.3",
    "vitest": "4.0.18"
  },
  "engines": {
    "node": ">=20.19.0"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
